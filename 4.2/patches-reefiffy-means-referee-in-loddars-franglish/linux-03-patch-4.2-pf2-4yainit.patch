+/*
+ * Check if only the current task is running on the cpu.
+ */
+bool single_task_running(void)
+{
+	return cpu_rq(smp_processor_id())->rq_running &&
+		(0 == queued_notrunning());
+}
+EXPORT_SYMBOL(single_task_running);
